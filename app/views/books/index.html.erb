<h1>Books</h1>

<table>
    <thead>
        <tr>
            <th>title</th>
            <th>body</th>
            <th></th>
        </tr>
    </thead>

<%= form_with model: @book do |f| %>
<h2>New Book</h2>
<h4>Title</h4>
<%= f.text_field :title %>
<h4>Body</h4>
<%= f.text_area :body %>
<div>
<%= f.submit 'Create Book' %>
</div>
<% end %>
<% @books.each do |book| %>

  <p>タイトル</p>
   <span>
     <%= link_to book.title, book_path(book.id) %>
  </span>
<% end %>